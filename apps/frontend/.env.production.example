# ── Production Environment Configuration ──

# ── API Configuration ──
VITE_API_BASE_URL=https://api.muejam.com/v1

# ── WebSocket Configuration ──
VITE_WS_URL=wss://api.muejam.com/ws

# ── Authentication ──
VITE_CLERK_PUBLISHABLE_KEY=pk_live_your-production-clerk-key

# ── Security ──
VITE_RECAPTCHA_SITE_KEY=your-production-recaptcha-key

# ── Push Notifications ──
VITE_VAPID_PUBLIC_KEY=your-production-vapid-key

# ── Feature Flags ──
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_SERVICE_WORKER=true

# ── Monitoring ──
VITE_ENABLE_SENTRY=true
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
VITE_ENABLE_PERFORMANCE_MONITORING=true
