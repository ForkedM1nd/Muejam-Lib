# ── Staging Environment Configuration ──

# ── API Configuration ──
VITE_API_BASE_URL=https://staging-api.muejam.com/v1

# ── WebSocket Configuration ──
VITE_WS_URL=wss://staging-api.muejam.com/ws

# ── Authentication ──
VITE_CLERK_PUBLISHABLE_KEY=pk_test_your-staging-clerk-key

# ── Security ──
VITE_RECAPTCHA_SITE_KEY=your-staging-recaptcha-key

# ── Push Notifications ──
VITE_VAPID_PUBLIC_KEY=your-staging-vapid-key

# ── Feature Flags ──
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_SERVICE_WORKER=false

# ── A/B Testing / Experiments ──
# Enable experimental features in staging
VITE_EXPERIMENTS=newDesign:true,betaFeature:true

# ── Monitoring ──
VITE_ENABLE_SENTRY=true
VITE_SENTRY_DSN=https://your-staging-sentry-dsn@sentry.io/project-id
VITE_ENABLE_PERFORMANCE_MONITORING=true
